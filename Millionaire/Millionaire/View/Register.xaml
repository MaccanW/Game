<Window x:Class="Millionaire.View.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Millionaire.View"
        mc:Ignorable="d"
        Title="Register" Height="300" Width="300">
    <Grid Background="#FF2389DE">
        <Button x:Name="button" Content="Register" HorizontalAlignment="Left" Margin="102,168,0,0" VerticalAlignment="Top" Width="90" Background="#FF2589DE" Click="button_Click"/>
        <TextBox x:Name="userNameTxt" HorizontalAlignment="Left" Height="23" Margin="91,55,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120" Foreground="White"/>
        <TextBox x:Name="passwordTxt" HorizontalAlignment="Left" Height="23" Margin="91,111,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120" Foreground="White"/>

    </Grid>
</Window>
