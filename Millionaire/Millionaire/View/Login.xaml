<Window x:Class="Millionaire.View.login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Millionaire.View"
        mc:Ignorable="d"
        Title="login" Height="305" Width="310">
    <Grid Background="#FF2389DE">
        <Border BorderBrush="Black" BorderThickness="0" Margin="32,20,32,27" Background="White" CornerRadius="3"/>
        <TextBox x:Name="usernameTxt" Height="27" Margin="86,91,86,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="loginBtn" Content="Log in" Margin="108,188,109,50" Click="loginBtn_Click" Background="#FF2389DE" Height="Auto" BorderBrush="#FF2389DE"/>
        <Label x:Name="label" Content="Logga in&#xD;&#xA;" Margin="101,26,100,0" VerticalAlignment="Top" RenderTransformOrigin="-5.259,1.014" FontSize="18" FontWeight="Bold" Height="37"/>
        <Label x:Name="label1_Copy" Content="Lösenord:" Margin="85,119,100,0" VerticalAlignment="Top" Height="29"/>
        <Label x:Name="label1" Content="Användarnamn:" HorizontalAlignment="Left" Margin="85,63,0,174"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="85,148,0,0" VerticalAlignment="Top" Width="131"/>

    </Grid>
</Window>
